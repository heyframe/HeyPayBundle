{
  "name": "heyframe/pay-bundle",
  "type": "symfony-bundle",
  "keywords": [
    "symfony",
    "payment",
    "heyframe",
    "alipay",
    "wechat pay"
  ],
  "license": "Apache-2.0",
  "require": {
    "php": "~8.2.0 || ~8.3.0 || ~8.4.0",
    "symfony/framework-bundle": "~7.3.0",
    "symfony/validator": "~7.3.0",
    "symfony/security-csrf": "~7.3.0",
    "symfony/dependency-injection": "~7.3.0",
    "php-http/message": "^1.16",
    "php-di/php-di": "^7.1",
    "league/uri": "^7.5",
    "league/uri-components": "^7.5",
    "psr/container": "^2.0",
    "php-http/discovery": "^1.20",
    "alipaysdk/openapi": "^3.0",
    "wechatpay/wechatpay": "^1.4",
    "symfony/property-access": "^7.3",
    "twig/twig": "^3.21.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.5.19",
    "doctrine/dbal": "^4.3.1",
    "doctrine/orm": "^3.5",
    "doctrine/persistence": "^4.1",
    "symfony/phpunit-bridge": "~7.3.0",
    "symfony/http-client": "~7.3.0",
    "symfony/var-dumper": "~7.3.0",
    "friendsofphp/php-cs-fixer": "3.82.1"
  },
  "scripts": {
    "ecs": "php-cs-fixer fix --dry-run --diff",
    "ecs-fix": "php-cs-fixer fix"
  },
  "autoload": {
    "psr-4": {
      "HeyPay\\Bundle\\PayBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "HeyPay\\Bundle\\PayBundle\\Tests\\": "Tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-main": "2.5-dev"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true,
      "symfony/runtime": true
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
